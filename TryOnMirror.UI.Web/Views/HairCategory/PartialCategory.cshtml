@model SymaCord.TryOnMirror.Entities.HairCategory

<li><span>@Model.CategoryName</span>
    <a href="@Url.Action("edit", "haircategory", new {id = Model.CategoryId})" class="edit" title="Edit">
        <i class="icon-pencil-2"></i>
    </a>
    <a href="@Url.Action("delete", "haircategory", new {id = Model.CategoryId})" class="delete" title="Delete">
        <i class="icon-remove-3"></i>
    </a>
</li>